[general]
compat-delete='delete-sequence'
display-n=-1
display-tab-names=0
gtk-use-system-default-theme=true
history-size=1000
max-tab-name-length=100
mouse-display=false
open-tab-cwd=true
prompt-on-quit=true
quick-open-command-line='gedit %(file_path)s'
restore-tabs-notify=true
restore-tabs-startup=true
save-tabs-when-changed=true
schema-version='3.8.1'
scroll-keystroke=true
start-fullscreen=false
use-default-font=true
use-popup=true
use-scrollbar=true
use-trayicon=true
window-halignment=0
window-height=100
window-losefocus=false
window-refocus=true
window-tabbar=true
window-width=100

[keybindings/global]
show-hide='F1'

[style/background]
transparency=100

[style/font]
allow-bold=true
palette='#000000000000:#ffff55555555:#5050fafa7b7b:#f1f1fafa8c8c:#bdbd9393f9f9:#ffff7979c6c6:#8b8be9e9fdfd:#bbbbbbbbbbbb:#555555555555:#ffff55555555:#5050fafa7b7b:#f1f1fafa8c8c:#bdbd9393f9f9:#ffff7979c6c6:#8b8be9e9fdfd:#ffffffffffff:#f8f8f8f8f2f2:#2e2e34343636'
palette-name='Custom'
